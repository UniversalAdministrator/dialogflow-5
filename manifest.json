{
  "manifest_version": 2,
  "name": "DialogFlow Steroids",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": [
        "https://console.dialogflow.com/api-client/*"
      ],
      "js": [
        "jquery-1.12.4.min.js",
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}